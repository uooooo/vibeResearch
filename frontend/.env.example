# Example environment variables (do not commit real secrets)

# OpenAI / Google LLM
OPENAI_API_KEY=
GOOGLE_API_KEY=
USE_REAL_LLM=1
USE_AI_SDK=1
USE_LLM_DEBUG=0

# OpenRouter (OpenAI-compatible proxy; preferred)
OPENROUTER_API_KEY=
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
# Optional but recommended
OPENROUTER_MODEL=openai/gpt-4o-mini
OPENROUTER_REFERRER=
OPENROUTER_TITLE=vibeResearch (local)

# Supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# App configuration
NEXT_PUBLIC_APP_NAME=vibeResearch
NEXT_PUBLIC_DEFAULT_DOMAIN=economics
APP_ALLOWED_ORIGINS=

# Optional providers
SEMANTIC_SCHOLAR_API_KEY=
PERPLEXITY_API_KEY=
